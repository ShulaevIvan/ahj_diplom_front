{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    "no-param-reassign": [2, { "props": false }],
    "prefer-destructuring": ["error", {"object": false, "array": false}],
     "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-unused-vars": "off",
    "no-alert": "off"
    }
}